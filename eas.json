{
  "cli": {
    "version": ">= 3.40.0",
    "appVersionSource": "remote",
    "requireCommit": true
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": { "buildType": "apk" },
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": { "buildType": "app-bundle" },
      "distribution": "store"
    }
  },
  "submit": {
    "production": {}
  }
}
